#: gnulib/lib/regcomp.c:135
msgid "Success"
msgstr "വിജയിച്ചു"

#: gnulib/lib/regcomp.c:138
msgid "No match"
msgstr "ചേരുന്നതില്ല"

#: gnulib/lib/regcomp.c:141
msgid "Invalid regular expression"
msgstr "റെഗുലര്‍ എക്സ്പ്രെഷന്‍ തെറ്റാണു്"

#: gnulib/lib/regcomp.c:144
